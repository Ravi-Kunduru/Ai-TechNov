pipeline {
    agent any

    stages {
        stage('food') {
            steps {
                sh 'docker build -t food .'
                echo 'build completed'
            }
        }
        stage('docker conatainer run')
            steps {
                sh 'docker run -d --name food -p 200:80 food'
                echo 'created conatiner'
            }
        }
    }
}
